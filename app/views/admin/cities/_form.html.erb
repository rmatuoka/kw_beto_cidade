    <div id="Box_register_form" class="twelve columns">
    
<%= form_for [:admin, @city] do |f| %>
  <%= f.error_messages %>
  <div class="field">
    <%= f.label :name, "Nome" %><br />
    <%= f.text_field :name, :placeholder => "Nome" %>
  </div>
  <div class="field">
    <%= f.label :endprefeitura, "Endereço da Prefeitura" %><br />
    <%= f.text_field :endprefeitura, :placeholder => "Endereço da Prefeitura" %>
  </div>
  <div class="field">
    <%= f.label :endcamara, "Endereço da Câmara Municipal" %><br />
    <%= f.text_field :endcamara, :placeholder => "Endereço da Câmara Municipal" %>
  </div>
  <div class="field">
    <%= f.label :fundacao, "Aniversário da Cidade" %><br />
    <%= f.date_select :fundacao %>
  </div>
  <div class="field">
    <%= f.label :nomepadroeiro, "Nome do Padroeiro" %><br />
    <%= f.text_field :nomepadroeiro, :placeholder => "Nome do Padroeiro" %>
  </div>
  <div class="field">
    <%= f.label :eventopadroeiro, "Data de evento do Padroeiro" %><br />
    <%= f.date_select :eventopadroeiro %>
  </div>
  <div class="field">
    <%= f.label :eventoscidade, "Nome do Padroeiro" %><br />
    <%= f.text_area :eventoscidade, :placeholder => "Datas/eventos comemorativos da cidade" %>
  </div>
  <div class="field">
    <%= f.label :prefeito, "Prefeito" %><br />
    <%= f.text_field :prefeito, :placeholder => "Prefeito" %>
  </div>
  <div class="field">
    <%= f.label :viceprefeito, "Vice-Prefeito" %><br />
    <%= f.text_field :viceprefeito, :placeholder => "Vice-Prefeito" %>
  </div>
  <div class="field">
    <%= f.label :vereadores, "Vereadores" %><br />
    <%= f.text_area :vereadores, :placeholder => "Vereadores" %>
  </div>
  <div class="field">
    <%= f.label :presidentepv, "Presidente do PV local" %><br />
    <%= f.text_field :presidentepv, :placeholder => "Presidente do PV local" %>
  </div>
  <div class="field">
    <%= f.label :liderancas, "Lideranças" %><br />
    <%= f.text_area :liderancas, :placeholder => "Lideranças" %>
  </div>
  <div class="field">
    <%= f.label :saneamento, "Serviço de Saneamento" %><br />
    <%= f.text_field :saneamento, :placeholder => "Serviço de Saneamento" %>
  </div>
  <div class="field">
    <%= f.label :quantvotosdept, "Quantidade de votos Deputado última eleição" %><br />
    <%= f.text_field :quantvotosdept, :placeholder => "Quantidade de votos Deputado última eleição" %>
  </div>
  <div class="field">
    <%= f.label :cincofederais, "Relação dos cinco deputados federais mais votados" %><br />
    <%= f.text_area :cincofederais, :placeholder => "Relação dos cinco deputados federais mais votados" %>
  </div>
  <div class="field">
    <%= f.label :cincoestaduais, "Relação dos cinco deputados estaduais mais votados" %><br />
    <%= f.text_area :cincoestaduais, :placeholder => "Relação dos cinco deputados estaduais mais votados" %>
  </div>
  <div class="field">
    <%= f.label :population, "Números de habitantes" %><br />
    <%= f.text_field :population, :placeholder => "Números de habitantes" %>
  </div>
  <div class="field">
    <%= f.label :eleitores, "Números de eleitores" %><br />
    <%= f.text_field :eleitores, :placeholder => "Números de eleitores" %>
  </div>
  <div class="field">
    <%= f.label :regadministrativa, "Região Administrativa" %><br />
    <%= f.text_field :regadministrativa, :placeholder => "Região Administrativa" %>
  </div>
  <div class="field">
    <%= f.label :vocacaoturistica, "Vocação Turística" %><br />
    <%= f.text_field :vocacaoturistica, :placeholder => "Vocação Turística" %>
  </div>
  <div class="field">
    <%= f.label :entirelacionamento, "Entidades com relacionamento" %><br />
    <%= f.text_field :entirelacionamento, :placeholder => "Entidades com relacionamento" %>
  </div>
  <div class="field">
    <%= f.label :histatuacao, "Histórico de Atuação/Emenda" %><br />
    <%= f.text_area :histatuacao, :placeholder => "Histórico de Atuação/Emenda" %>
  </div>
  <div class="field">
    <%= f.label :entianimal, "Entidade de Bem-estar Animal" %><br />
    <%= f.text_area :entianimal, :placeholder => "Entidade de Bem-estar Animal" %>
  </div>
  <div class="field">
    <%= f.label :verdeazul, "Município Verde /Azul: (S/N)" %><br />
    <%= f.check_box :verdeazul %>
  </div>
  <div class="field">
    <%= f.label :estrutmeioambiente, "Qual estrutura da Administração Municipal de Meio Ambiente" %><br />
    <%= f.text_area :estrutmeioambiente, :placeholder => "Qual estrutura da Administração Municipal de Meio Ambiente" %>
  </div>
  <div class="field">
    <%= f.label :estrutturismo, "Qual estrutura da Administração Municipal de Turismo" %><br />
    <%= f.text_area :estrutturismo, :placeholder => "Qual estrutura da Administração Municipal de Turismo" %>
  </div>
  <div class="field">
    <%= f.label :estrutcamara, "Qual estrutura da Administração Municipal de Cultura" %><br />
    <%= f.text_area :estrutcamara, :placeholder => "Qual estrutura da Administração Municipal de Cultura" %>
  </div>
  <div class="field">
    <%= f.label :estrutbemanimal, "Qual estrutura da Administração Municipal de Bem-estar Animal" %><br />
    <%= f.text_area :estrutbemanimal, :placeholder => "Qual estrutura da Administração Municipal de Bem-estar Animal" %>
  </div>
  <p><%= f.submit "Salvar", :class => "button" %></p>
<% end %>
</div>
